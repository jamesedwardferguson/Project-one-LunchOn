
<%= form_for(@lunchitem, :html => {:multipart => true}) do |f| %>
  <p>
    <%= f.label :image %>
    <%= f.cl_image_upload :image, :required => true %>
  </p>
  <p> LunchOn
    <%= f.label :name %>
    <%= f.text_field :name, :required => true %>
  </p>
  <p>
    <%= f.label :price %>
    <%= f.number_field :price, :required => true %>
  </p>
  <%= f.fields_for :location do |location| %>
    <p>Store
      <%= location.label :name %>
      <%= location.text_field :name %>
    </p>
    <p>Store
      <%= location.label :address %>
      <%= location.text_field :address %>
    </p>
  <% end %>
  <%= f.submit %>
<% end %>
